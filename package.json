{
  "name": "ml_2019",
  "version": "0.0.1",
  "description": "ML test, made using my Boilerplate for ReactProjects (https://gitlab.com/venturalp/reactboilerplate.git)",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "start:prd": "cross-env NODE_ENV=production webpack --mode production --progress --inline --color && webpack-dev-server --mode production --open",
    "build": "npm run client:build && npm run server:build",
    "client:build": "cross-env NODE_ENV=production webpack --config webpack.config.client.js --mode production --progress --inline --color",
    "server:build": "cross-env NODE_ENV=production webpack --config webpack.config.server.js --mode production --progress --inline --color",
    "client:dev": "cross-env NODE_ENV=development webpack --config webpack.config.client.js --mode development --progress --inline --color --watch",
    "server:dev": "cross-env NODE_ENV=development webpack --config webpack.config.server.js --mode development --progress --inline --color --watch",
    "build:dev": "npm-run-all --parallel client:dev server:dev",
    "server": "nodemon deploy/server.js",
    "start": "npm-run-all --parallel build:dev server",
    "dev-server": "webpack-dev-server --config webpack.config.client.js --mode development --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venturalp/ML_2019"
  },
  "author": "Guilherme Ventura de Souza",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@svgr/cli": "^4.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "prettier": "^1.17.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "es6-promise": "^4.2.6",
    "express": "^4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "load-json-file": "^6.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-loading": "^2.0.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
